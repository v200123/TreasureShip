<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="84dp"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<com.lc.liuchanglib.ShapeImageView
    android:id="@+id/iv_layout_image"
    android:layout_marginTop="8dp"
    app:qmui_border_width="1dp"
    app:qmui_border_color="@color/background_grey"
    tools:src="@drawable/icon_upload_add"
    android:layout_width="74dp"
    android:layout_height="74dp"/>
    <ImageView
        android:id="@+id/iv_layout_close"
        android:layout_gravity="end"
        android:src="@drawable/ico_alph_close"
        android:layout_width="22dp"
        android:layout_height="22dp"/>
</FrameLayout>